<p id="notice"><%= notice %></p>

<h2>Listing Experiments</h2>

<table>
	<thead>
		<tr>
			<th>Name</th>
			<th>Fullname</th>
			<th colspan="3"></th>
		</tr>
	</thead>

	<tbody>
	<% @settings_experiments.each do |settings_experiment| %>
		<tr>
		<td><%= settings_experiment.name %></td>
		<td><%= settings_experiment.fullname %></td>
		<td><%= button_to 'Show', settings_experiment, method: :get %></td>
		<% if can? :manage, :settings %>
			<td><%= button_to 'Edit', edit_settings_experiment_path(settings_experiment), method: :get %></td>
		<td><%= button_to 'Del', settings_experiment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
	<%end%>
	</tr>
<% end %>
	</tbody>
</table>

<br>

<%= button_to 'Add New Experiment', new_settings_experiment_path, method: :get %>
