
<script src="/d3/d3.v3.min.js"></script>
<script>
var w = 500;
var h = 500;

var svg = d3.select("body")
.append("svg")
.attr("width", w)   
.attr("height", h); 


var mySquare=svg.append("rect")
.attr("x",60)
.attr("y",60)
.attr("width",60)
.attr("height",60)
.attr("fill","red");

var button = d3.select("body")
.append("input")
.attr("type","button")
.attr("value", "Move");

button.on("click", function() {
		mySquare
		.transition()
		.attr("x",450)
		.transition()
		.delay(5000)
		.attr("x",60);
		
		})


</script>

