<div id="page-wrapper">

	<!-- Header -->

	<%= render "layouts/partials/header"%>

	<!-- Main -->
	<article id="main">
		<header>
			<h2>CDAAS.RIMES</h2>
			<p><font color=#fff>Climate Data Access & Analysis System @ RIMES</p>
			</header>
			<section class="wrapper style5">
				<div class="inner">


					<section>
						<div class="table-wrapper">
