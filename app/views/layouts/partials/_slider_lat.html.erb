<script>
$(function() {
		$( "#lat_range" ).slider({
range: true,
min: -90,
max: 90,
values: [ -90, 90 ],
slide: function( event, ui ) {
$( "#lat_amount" ).val(  ui.values[ 0 ] + " ~ " + ui.values[ 1 ] );
$( "#s_lat" ).val(  ui.values[ 0 ] );
$( "#e_lat" ).val(  ui.values[ 1 ] );
}
});
		$( "#lat_amount" ).val(  $( "#lat_range" ).slider( "values", 0 ) +
			" ~ " + $( "#lat_range" ).slider( "values", 1 ) );
		$( "#s_lat" ).val(  $( "#lat_range" ).slider( "values", 0 ));
		$( "#e_lat" ).val(  $( "#lat_range" ).slider( "values", 1 ));


		});
</script>
