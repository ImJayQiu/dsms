<p id="notice"><%= notice %></p>

<h2>Listing ECMWF Forcasting Types</h2>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Folder</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ecmwf_types.each do |ecmwf_type| %>
      <tr>
        <td><%= ecmwf_type.name %></td>
        <td><%= ecmwf_type.folder %></td>
        <td><%= link_to 'Show', ecmwf_type %></td>
        <td><%= link_to 'Edit', edit_ecmwf_type_path(ecmwf_type) %></td>
        <td><%= link_to 'Del', ecmwf_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Add New Forcasting Type', new_ecmwf_type_path %>
