<p id="notice"><%= notice %></p>

<h2>Listing Feedbacks</h2>

<table>
	<tr>
		<th>
			<%= will_paginate @feedbacks %>
		</th>
	</tr>
</table>
<table>
	<thead>
		<tr>
			<th>Title</th>
			<th>User</th>
			<th colspan="3"></th>
		</tr>
	</thead>

	<tbody>
	<% @feedbacks.each do |feedback| %>
		<tr>
		<td><%= feedback.title %></td>
		<td><%= feedback.user %></td>
		<td><%= link_to 'Show', feedback %></td>
		<td><%= link_to 'Edit', edit_feedback_path(feedback) %></td>
		<% if can? :manage, :feedback %>
			<td><%= button_to 'Del', feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
	<% end #can %>
	</tr>
<% end %>
	</tbody>
</table>

<table>
	<tr>
		<th>
			<%= will_paginate @feedbacks %>
		</th>
	</tr>
</table>
<br>

<%= button_to 'New Feedback', new_feedback_path, method: :get %>
